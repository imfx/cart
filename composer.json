{
    "name": "zrkb/cart",
    "description": "Laravel Shopping Cart",
    "keywords": [
        "laravel",
        "shopping",
        "cart"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Ayala",
            "email": "hi@felixaya.la"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/session": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/events": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cart\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Cart\\Facades\\Cart"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}